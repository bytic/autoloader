{
  "name": "bytic/autoloader",
  "description": "Autoloader component for bytic",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\AutoLoader\\": "src/"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\AutoLoader\\Tests\\": "tests/src",
      "Nip\\AutoLoader\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/logger": "^0.9|^1.0",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2|^8.0",
    "mockery/mockery": "^1.1"
  }
}
